{
  "name": "b2pweb/bdf-form-filter-prime",
  "description": "Helper library for BDF Form to create Prime filters",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vincent Quatrevieux"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bdf\\Form\\Filter\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bdf\\Form\\Filter\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "require": {
    "php": "~7.1 || ~8.0.0 || ~8.1.0",
    "b2pweb/bdf-prime": "~1.0|~2.0",
    "b2pweb/bdf-form": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0",
    "vimeo/psalm": "~4.4",
    "giggsey/libphonenumber-for-php": "~8.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "scripts": {
    "tests": "phpunit",
    "tests-with-coverage": "phpunit --coverage-clover coverage.xml",
    "psalm": "psalm --shepherd"
  }
}
